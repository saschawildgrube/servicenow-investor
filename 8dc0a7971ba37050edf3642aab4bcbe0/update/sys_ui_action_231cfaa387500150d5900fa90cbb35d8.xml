        <name>Alphavantage Symbol Search</name>
        <onclick/>
        <order>10000</order>
        <script><![CDATA[var alphavantage = new AlphaVantageAPI(AppGetProperty('api.alphavantage.apikey'));

var strSymbol = ''+current.symbol;

var grExchange = GetExchangeRecord(current.exchange);
if (grExchange != false)
{
	var strExchangeSymbol = ''+grExchange.symbol;
	if (strExchangeSymbol != '')
	{
		strSymbol += '.'+strExchangeSymbol;
	}
}
var result = alphavantage.SymbolSearch(strSymbol);
var strMessage = x_snc_devtools.RenderValue(result);
strMessage = x_snc_devtools.HtmlEncode(strMessage);
gs.addInfoMessage(strMessage);
x_snc_devtools.RedirectToRecord(current);]]></script>
